setenv PYGDALSAR           $prefix

prepend-path PYTHONPATH  $prefix/contrib/parsers
prepend-path PYTHONPATH  $prefix/contrib/python
prepend-path PATH  $prefix/AtmoCorr
prepend-path PATH  $prefix/bin
prepend-path PATH  $prefix/Timeseries
prepend-path PATH  $prefix/GeoTools
prepend-path PATH  $prefix/Pyrocko
prepend-path PATH  $prefix/Plots

set nsbas /data/soft/PyGdalSAR/NSBAS-playground
set exec_prefix ${nsbas}
prepend-path PATH  $nsbas/correction_tools
prepend-path PATH  $nsbas/utils
prepend-path PATH  $nsbas/plots
prepend-path PATH  $nsbas/pixel-tracking

